<?xml version="1.0"?>
<JWM>
  <!-- TRAY PROPERTIES -->
  
  <TrayStyle>
      <Font>Sans-12</Font>
      <Background>#F4F4F4</Background>
      <Foreground>#000000</Foreground>
      <Opacity>0.75</Opacity>
  </TrayStyle>
  
  
  <!-- Tray at the bottom. -->
  <Tray x="0" y="-1" autohide="off">

    <TrayButton label="Menu">root:1</TrayButton>
    <!--<TrayButton label="_">showdesktop</TrayButton>-->

    <Pager labeled="true"/>

    <TaskList maxwidth="256"/>

    <Dock/>
    <!-- <Clock><Button mask="123">exec:xclock</Button></Clock> -->
    <!-- <Clock format="[%F] %T"/> -->
    <Clock><Button mask="123">exec:xclock;pgrep gsimplecal > /dev/null &amp;&amp; killall gsimplecal || gsimplecal</Button></Clock>
      
  </Tray>
</JWM>
